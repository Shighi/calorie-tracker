  "dependencies": {
    "axios": "^1.8.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^7.2.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mongoose": "^8.12.1",
    "morgan": "^1.10.0",
    "nock": "^14.0.1",
    "pg": "^8.11.3",
    "redis": "^4.7.0",
    "sequelize": "^6.32.0",
    "swagger-ui-express": "^4.6.2",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "babel-jest": "^29.7.0",
    "chai": "^4.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "sinon": "^17.0.1",
    "supertest": "^6.3.3"
